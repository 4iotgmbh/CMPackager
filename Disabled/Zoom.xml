<ApplicationDef>
	<Application>
		<Name>Zoom</Name>
		<Description>Video Conferencing Application. Account Required</Description>
		<Publisher>Zoom Video Communications Inc.</Publisher>
		<AutoInstall>True</AutoInstall>
		<UserDocumentation>https://zoom.us/</UserDocumentation>
		<Icon>Zoom.ico</Icon>
	</Application>
	<Downloads>
		<Download DeploymentType="DeploymentType1">
			<PrefetchScript></PrefetchScript>
			<URL>https://zoom.us/client/latest/ZoomInstallerFull.msi</URL>
			<DownloadFileName>ZoomInstallerFull.msi</DownloadFileName>
			<Version></Version>
			<FullVersion></FullVersion>
			<DownloadVersionCheck>
			$Version = ([String](Get-MSIInfo -Path $DownloadFile -Property ProductVersion)).TrimStart().TrimEnd()
			#.TrimStart().TrimEnd())
			</DownloadVersionCheck>
			<ExtraCopyFunctions></ExtraCopyFunctions>
		</Download>
	</Downloads>
	<DeploymentTypes>
		<DeploymentType Name="DeploymentType1">
			<DeploymentTypeName>Zoom Silent Installer</DeploymentTypeName>
			<InstallationType>MSI</InstallationType>
			<CacheContent>False</CacheContent>
			<BranchCache>True</BranchCache>
			<ContentFallback>True</ContentFallback>
			<OnSlowNetwork>Download</OnSlowNetwork>
			<InstallProgram>msiexec /i ZoomInstallerFull.msi /quiet /qn /norestart ZoomAutoUpdate=`"true`" ZConfig=`"nogoogle=1;nofacebook=1;AddFWException=1`"</InstallProgram>
			<InstallationMSI>ZoomInstallerFull.msi</InstallationMSI>
			<UninstallCmd></UninstallCmd>
			<InstallationBehaviorType>InstallForSystem</InstallationBehaviorType>
			<LogonReqType>WhetherOrNotUserLoggedOn</LogonReqType>
			<UserInteractionMode>Hidden</UserInteractionMode>
			<EstRuntimeMins>15</EstRuntimeMins>
			<MaxRuntimeMins>60</MaxRuntimeMins>
			<RebootBehavior>BasedOnExitCode</RebootBehavior>
			<DetectionMethodType>MSI</DetectionMethodType>
		</DeploymentType>
	</DeploymentTypes>
	<Distribution>
		<DistributeContent>True</DistributeContent>
	</Distribution>
	<Supersedence>
		<Supersedence>True</Supersedence>
		<Uninstall>False</Uninstall>
		<CleanupSuperseded>True</CleanupSuperseded>
		<KeepSuperseded>2</KeepSuperseded>
	</Supersedence>
	<Deployment>
		<DeploySoftware>True</DeploySoftware>
		<Purpose>Required</Purpose>
		<DeploymentCollection>Zoom-Test</DeploymentCollection>
	</Deployment>
	<Deployment>
		<DeploySoftware>True</DeploySoftware>
		<Purpose>Required</Purpose>
		<AvailableOffset>7.00:00:00</AvailableOffset>
		<DeadlineOffset>14.00:00:00</DeadlineOffset>
		<DeploymentCollection>Zoom-Pilot</DeploymentCollection>
	</Deployment>
	<Deployment>
		<DeploySoftware>True</DeploySoftware>
		<Purpose>Required</Purpose>
		<AvailableOffset>14.00:00:00</AvailableOffset>
		<DeadlineOffset>28.00:00:00</DeadlineOffset>
		<DeploymentCollection>Zoom-General Availability</DeploymentCollection>
	</Deployment>
</ApplicationDef>